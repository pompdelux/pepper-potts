<script type='text/javascript'>
$(function() {
  Dashing.widget_base_dimensions = [340, 260]
  Dashing.numColumns = 5
});
</script>

<% content_for :title do %>Hvuddn har PDL det ?<% end %>

<div class="gridster tv">
    <ul>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
          <div data-view="Clock"></div>
          <i class="icon-time icon-background"></i>
        </li>
        <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
          <div data-id="lastdeploy" data-view="Text" data-title="Seneste deploy" data-moreinfo="" data-text="" style="background-color:#96bf48;"></div>
          <i class="icon-heart icon-background"></i>
        </li>
        <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
          <div data-view="Graphite" data-image="http://pdlstats1.pompdelux.com/render/?width=340&height=260&_salt=1418895149.559&from=-90minutes&connectedLimit=&hideAxes=false&hideYAxis=false&hideGrid=false&target=alias(summarize(sumSeries(stats.counters.prod.order.*DK.count)%2C%2215min%22)%2C%22*DK%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*DE.count)%2C%2215min%22)%2C%22*DE%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.COM.count)%2C%2215min%22)%2C%22*COM%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*CH.count)%2C%2215min%22)%2C%22*CH%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*FI.count)%2C%2215min%22)%2C%22*FI%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*NL.count)%2C%2215min%22)%2C%22*NL%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*SE.count)%2C%2215min%22)%2C%22*NO%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*.count)%2C%2215min%22)%2C%22ALL%22)&target=alias(summarize(sumSeries(stats.counters.prod.order.*AT.count)%2C%2215min%22)%2C%22*AT%22)&title=Orders%20pr.%20Country" data-interval="60000"></div>
        </li>

        <li data-row="1" data-col="2" data-sizex="2" data-sizey="1">
            <div data-id="welcome" data-view="Text" data-title="Hello" data-text="This is your shiny new dashboard." data-moreinfo="Husk at flushe caches før hver af disse datoer."></div>
        </li>

        <li data-row="2" data-col="2" data-sizex="1" data-sizey="2">
          <div data-id="visitor-count" data-view="List" data-unordered="true" data-title="Online brugere" data-moreinfo="opdateres én gang i minuttet"></div>
          <i class="icon-bar-chart icon-background"></i>
        </li>

        <li data-row="2" data-col="3" data-sizex="1" data-sizey="2">
            <div data-id="ordercount" data-view="List" data-title="Antal ordre" data-unordered="true" data-moreinfo="seneste time"></div>
            <i class="icon-bar-chart icon-background"></i>
        </li>

        <li data-row="1" data-col="4" data-sizex="1" data-sizey="1">
            <div data-id="travis-master" data-view="Travis" data-unordered="true" data-title="Hanzo / master"></div>
        </li>
        <li data-row="2" data-col="4" data-sizex="1" data-sizey="1">
            <div data-id="travis-other" data-view="Travis" data-unordered="true" data-title="Hanzo / *"></div>
        </li>
        <li data-row="3" data-col="4" data-sizex="1" data-sizey="1">
          <div data-view="Graphite" data-image="http://pdlstats1.pompdelux.com/render/?width=340&height=260&_salt=1418734848.266&from=-90minutes&title=Avg%20page%20response%20time%20-%20from%20server&lineMode=staircase&areaMode=first&yStep=&logBase=&yMax=&yMin=&target=alias(removeAboveValue(averageSeries(stats.timers.prod.buildtime.*.mean)%2C2000)%2C%22Avg%20response%20time%20%3C%202000ms%22)" data-interval="60000"></div>
        </li>

    </ul>
</div>
