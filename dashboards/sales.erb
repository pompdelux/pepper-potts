<script type='text/javascript'>
$(function() {
  Dashing.widget_base_dimensions = [340, 260]
  Dashing.numColumns = 5
});
</script>

<% content_for :title do %>Hvuddn har PDL det ?<% end %>

<div class="gridster tv">
    <ul>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
          <div data-view="Clock"></div>
          <i class="icon-time icon-background"></i>
        </li>

        <li data-row="2" data-col="1" data-sizex="1" data-sizey="2">
            <div data-id="ordercount" data-view="List" data-title="Antal ordre" data-unordered="true" data-moreinfo="seneste time"></div>
            <i class="icon-bar-chart icon-background"></i>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="3">
            <div data-id="topsellers-girl" data-view="List" data-unordered="true" data-title="Top sælgere (G)" data-moreinfo="stk solgt pr. style i dk - seneste 24 timer" style="background-color:#dc5945;"></div>
            <i class="icon-bar-chart icon-background"></i>
        </li>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="3">
            <div data-id="topsellers-boy" data-view="List" data-unordered="true" data-title="Top sælgere (B)" data-moreinfo="stk solgt pr. style i dk - seneste 24 timer"></div>
            <i class="icon-bar-chart icon-background"></i>
        </li>
        <li data-row="1" data-col="1" data-sizex="1" data-sizey="3">
            <div data-id="topsellers-little" data-view="List" data-unordered="true" data-title="Top sælgere (LB/LG)" data-moreinfo="stk solgt pr. style i dk - seneste 24 timer" style="background-color:#96bf48;"></div>
            <i class="icon-bar-chart icon-background"></i>
        </li>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="3">
          <div data-id="visitor-count" data-view="List" data-unordered="true" data-title="Online brugere" data-moreinfo="opdateres én gang i minuttet"></div>
          <i class="icon-bar-chart icon-background"></i>
        </li>
    </ul>
</div>
